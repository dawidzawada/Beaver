{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@screen/*": ["src/screen/*"],
      "@shared/*": ["src/shared/*"],
      "@domain/*": ["src/domain/*"],
      "@store/*": ["src/store/*"],
      "@src/*": ["src/*"]
    }
  },
  "include": ["./src/**/*", "./src/**/*.ts", "./src/**/*.tsx"],
  "exclude": ["_old"]
}
